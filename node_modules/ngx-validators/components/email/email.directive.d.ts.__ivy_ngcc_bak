import { EmailOptions } from './email-util';
import { OnInit } from '@angular/core';
import { Validator, AbstractControl } from '@angular/forms';
export declare class EmailValidatorDirective implements Validator, OnInit {
    email: string;
    private validator;
    ngOnInit(): void;
    validate(c: AbstractControl): {
        [key: string]: any;
    };
}
export declare class EmailSuggestValidatorDirective implements Validator, OnInit {
    emailSuggest: EmailOptions;
    private validator;
    ngOnInit(): void;
    validate(c: AbstractControl): {
        [key: string]: any;
    };
}
